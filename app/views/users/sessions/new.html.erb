<div class="col-xs-1">
</div>

<div class="col-xs-10">
  <div class="main-header">
    <h2>ログイン</h2>
  </div>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <table class="table table-bordered">
        <tbody>
          <tr>
             <th><%= f.label "メールアドレス" %></th>
            <td><%= f.email_field :email, autofocus: true, autocomplete: "メールアドレス", :size=>"50" %></td>
          </tr>
          <tr>
            <th><%= f.label "パスワード" %></th>
            <td><%= f.password_field :password, autocomplete: "パスワード", :size=>"50" %></td>
          </tr>
        </tbody>
      </table>
      <div class="actions">
        <%= f.submit "ログイン" %>
      </div>
    <% end %>
</div>

<div class="col-xs-1">
</div>

